{
  "name": "rc-hooks",
  "version": "2.2.2",
  "description": "React Hooks Library.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "npm run dev",
    "dev": "dumi dev",
    "build": "npm run build:es && npm run build:cjs && npm run build:umd",
    "build:es": "rm -rf es && tsc -m ES2015 -t ES5 --outDir es -d",
    "build:cjs": "rm -rf lib && tsc -m commonjs -t ES5 --outDir lib -d",
    "build:umd": "rm -rf dist && rollup -c",
    "doc": "dumi build",
    "doc-version": "cross-env BUIDL_DOC_VERSION=true dumi build",
    "prettier": "prettier --write **/*",
    "lint-staged": "lint-staged",
    "precommit": "lint-staged",
    "lint": "eslint src --ext ts,tsx",
    "lint:fix": "npm run lint -- --fix",
    "commit": "cz",
    "test": "jest --verbose",
    "prepare": "husky install",
    "prepublishOnly": "npm test && npm run build"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": "eslint",
    "**/*.{js,jsx,ts,tsx,json,css,scss,sass,less,md}": "prettier -w"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "files": [
    "dist",
    "es",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doly-dev/rc-hooks.git"
  },
  "keywords": [
    "react",
    "hook"
  ],
  "author": "caijf",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/doly-dev/hooks/issues"
  },
  "homepage": "https://github.com/doly-dev/hooks#readme",
  "dependencies": {
    "@types/lodash": "^4.14.184",
    "lodash": "^4.17.21",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/plugin-proposal-private-property-in-object": "^7.18.6",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.1.1",
    "@commitlint/config-conventional": "^17.1.0",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.4.0",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.8",
    "@types/mockjs": "^1.0.6",
    "@types/node": "^18.7.13",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "antd": "^4.22.8",
    "babel-jest": "^29.0.1",
    "babel-plugin-import": "^1.13.5",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dayjs": "^1.11.5",
    "dumi": "^1.1.47",
    "dumi-theme-default": "^1.1.23",
    "eslint": "^8.23.0",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest": "^29.0.1",
    "jest-environment-jsdom": "^29.0.1",
    "lint-staged": "^13.0.3",
    "mockjs": "^1.1.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.78.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2",
    "util-helpers": "^4.12.5"
  },
  "peerDependencies": {
    "react": ">= 16.8.6"
  }
}
