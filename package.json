{
  "name": "rc-hooks",
  "version": "0.8.0",
  "description": "React Hooks Library.",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "sideEffects": false,
  "types": "types/index.d.ts",
  "scripts": {
    "start": "npm run dev",
    "dev": "npx dumi dev",
    "build": "npm run build:lib & npm run build:esm",
    "build:lib": "rm -rf lib && cross-env MODULE_TYPE=lib babel src --out-dir lib --ignore 'src/**/demo/**/*','src/**/*.md','src/.umi','src/.umi-production'",
    "build:esm": "rm -rf esm && cross-env MODULE_TYPE=esm babel src --out-dir esm --ignore 'src/**/demo/**/*','src/**/*.md','src/.umi','src/.umi-production'",
    "build:doc": "npm run doc",
    "doc": "npx dumi build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doly-dev/rc-hooks.git"
  },
  "keywords": [
    "react",
    "hook"
  ],
  "author": "caijf",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/doly-dev/hooks/issues"
  },
  "homepage": "https://github.com/doly-dev/hooks#readme",
  "dependencies": {
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@typescript-eslint/parser": "^1.13.0",
    "antd": "^4.1.0",
    "babel-plugin-import": "^1.13.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.8.29",
    "dumi": "^1.0.34",
    "eslint-plugin-react": "^7.20.3",
    "mockjs": "^1.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "typescript": "^3.5.1"
  },
  "peerDependencies": {
    "react": "^16.8.6"
  }
}
